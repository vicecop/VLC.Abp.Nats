  <ItemGroup>
    <PackageReference Include="STAN.Client" Version="0.3.0" />
    <PackageReference Include="Volo.Abp.Json" Version="3.3.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

